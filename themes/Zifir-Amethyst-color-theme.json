{
	"name": "Zifir Amethyst Pastel",
	"colors": {
	  // Editor
	  "editor.background": "#101010",
	  "editor.foreground": "#FFFFFF",
	  "editor.selectionBackground": "#FFFFFF25",
	  "editor.selectionHighlightBackground": "#FFFFFF25",
	  "editorLineNumber.foreground": "#505050",
	  "editorGroupHeader.tabsBackground": "#101010",
	  "editorWidget.background": "#101010",
	  "editorWarning.foreground": "#FFB74D", // Pastel Portakal
	  "editorError.foreground": "#E57373",   // Pastel Kırmızı
	  "editorOverviewRuler.border": "#101010",
	  "editorGutter.addedBackground": "#B2DFDB", // Pastel Akuamarin (string/ekleme rengi)
	  "editorGutter.deletedBackground": "#E57373", // Pastel Kırmızı
	  "editorGutter.modifiedBackground": "#FFB74D", // Pastel Portakal
	  "diffEditor.insertedTextBackground": "#B2DFDB1A", // Pastel Akuamarin alpha
	  "diffEditor.insertedLineBackground": "#B2DFDB1A",
	  "diffEditor.removedTextBackground": "#E573731A",  // Pastel Kırmızı alpha
	  "diffEditor.removedLineBackground": "#E573731A",
	  "editorInlayHint.foreground": "#A0A0A0",
	  "editorInlayHint.background": "#1C1C1C",
	  // Sidebar
	  "sideBar.background": "#101010",
	  "sideBarTitle.foreground": "#A0A0A0",
	  "sideBarSectionHeader.foreground": "#A0A0A0",
	  "sideBarSectionHeader.background": "#101010",
	  // Activity bar
	  "activityBar.background": "#101010",
	  "activityBar.foreground": "#A0A0A0",
	  "activityBarBadge.background": "#C5B4E3", // YENİ ANA VURGU (Pastel Lavanta)
	  "activityBarBadge.foreground": "#333333", // Koyu gri metin
	  // Title bar
	  "titleBar.activeBackground": "#101010",
	  "titleBar.inactiveBackground": "#101010",
	  "titleBar.activeForeground": "#7E7E7E",
	  "titleBar.inactiveForeground": "#707070",
	  // Tab
	  "tab.border": "#101010",
	  "tab.activeBackground": "#161616",
	  "tab.activeBorder": "#C5B4E3", // YENİ ANA VURGU (Pastel Lavanta)
	  "tab.inactiveBackground": "#101010",
	  // Status bar
	  "statusBar.debuggingForeground": "#FFFFFF",
	  "statusBar.debuggingBackground": "#B71C1C", // Koyu pastel kırmızı
	  "statusBar.background": "#101010",
	  "statusBar.foreground": "#A0A0A0",
	  "statusBarItem.remoteBackground": "#C5B4E3", // YENİ ANA VURGU (Pastel Lavanta)
	  "statusBarItem.remoteForeground": "#333333", // Koyu gri metin
	  // Change explorer active item color
	  "list.activeSelectionForeground": "#C5B4E3", // YENİ ANA VURGU (Pastel Lavanta)
	  "list.inactiveSelectionBackground": "#232323",
	  // Global
	  "badge.background": "#C5B4E3", // YENİ ANA VURGU (Pastel Lavanta)
	  "badge.foreground": "#333333", // Koyu gri metin
	  "button.background": "#C5B4E3", // YENİ ANA VURGU (Pastel Lavanta)
	  "button.hoverBackground": "#D0BFE9", // Pastel Lavantanın biraz açığı
	  "button.foreground": "#333333", // Koyu gri metin
	  "focusBorder": "#C5B4E3", // YENİ ANA VURGU (Pastel Lavanta)
	  "icon.foreground": "#A0A0A0",
	  "input.background": "#1C1C1C",
	  "list.activeSelectionBackground": "#232323",
	  "list.hoverBackground": "#282828",
	  "list.errorForeground": "#E57373",   // Pastel Kırmızı
	  "list.highlightForeground": "#C5B4E3", // YENİ ANA VURGU (Pastel Lavanta)
	  "selection.background": "#666666",
	  // Brackets
	  "editorBracketHighlight.foreground1": "#A0A0A0",
	  "editorBracketHighlight.foreground2": "#A0A0A0",
	  "editorBracketHighlight.foreground3": "#A0A0A0",
	  "editorBracketHighlight.foreground4": "#A0A0A0",
	  "editorBracketHighlight.foreground5": "#A0A0A0",
	  "editorBracketHighlight.foreground6": "#A0A0A0",
	  "editorBracketHighlight.unexpectedBracket.foreground": "#E57373",
	  // Links
	  "textLink.foreground": "#C5B4E3", // YENİ ANA VURGU (Pastel Lavanta)
	  "textLink.activeForeground": "#D0BFE9", // Pastel Lavantanın biraz açığı
	  // Tooltips
	  "editorHoverWidget.background": "#161616",
	  "editorHoverWidget.border": "#282828",
	  // Scrollbar
	  "scrollbarSlider.background": "#34343480",
	  "scrollbarSlider.hoverBackground": "#343434",
	  // Settings
	  "settings.modifiedItemIndicator": "#C5B4E3" // YENİ ANA VURGU (Pastel Lavanta)
	},
	"tokenColors": [
	  {
		"name": "Comment",
		"scope": ["comment", "punctuation.definition.comment"],
		"settings": {
		  "foreground": "#8b8b8b94"
		}
	  },
	  {
		"name": "Variables",
		"scope": [
		  "variable",
		  "string constant.other.placeholder"
		],
		"settings": {
		  "foreground": "#FFFFFF"
		}
	  },
	  {
		"name": "Colors",
		"scope": ["constant.other.color"],
		"settings": {
		  "foreground": "#FFFFFF"
		}
	  },
	  {
		"name": "Invalid",
		"scope": ["invalid", "invalid.illegal"],
		"settings": {
		  "foreground": "#E57373" // Pastel Kırmızı
		}
	  },
	  {
		"name": "Keyword, Storage, Operator, Punctuation", // KONTRAST İÇİN GRİ
		"scope": [
		  "keyword",
		  "storage.type",
		  "storage.modifier",
		  "keyword.operator",
		  "punctuation.definition.tag",
		  "punctuation.separator",
		  "punctuation.accessor"
		],
		"settings": {
		  "foreground": "#A0A0A0"
		}
	  },
	  {
		"name": "Control Keywords",
		"scope": ["keyword.control"],
		"settings": {
		  "foreground": "#A0A0A0"
		}
	  },
	  {
		"name": "Tag Name",
		"scope": [
		  "entity.name.tag",
		  "meta.tag.sgml"
		],
		"settings": {
		  "foreground": "#C5B4E3" // YENİ ANA VURGU (Pastel Lavanta)
		}
	  },
	  {
		"name": "Function and Method Names, Class Names, Type Names",
		"scope": [
		  "entity.name.function",
		  "variable.function",
		  "support.function",
		  "entity.name.class",
		  "entity.name.type",
		  "support.class",
		  "support.type"
		],
		"settings": {
		  "foreground": "#C5B4E3" // YENİ ANA VURGU (Pastel Lavanta)
		}
	  },
	  {
		"name": "Special Methods, Namespace",
		"scope": [
		  "keyword.other.special-method",
		  "support.other.namespace.use.php",
		  "meta.use.php",
		  "support.other.namespace.php",
		  "support.type.sys-types"
		],
		"settings": {
		  "foreground": "#C5B4E3" // YENİ ANA VURGU (Pastel Lavanta)
		}
	  },
	  {
		"name": "Number, Boolean, Null, Constants",
		"scope": [
		  "constant.numeric",
		  "constant.language.boolean",
		  "constant.language.null",
		  "support.constant",
		  "constant.character",
		  "constant.escape",
		  "keyword.other.unit"
		],
		"settings": {
		  "foreground": "#B2DFDB" // YENİ STRİNG RENGİ (Pastel Akuamarin)
		}
	  },
	  {
		"name": "String, Symbols, Keys",
		"scope": [
		  "string",
		  "constant.other.symbol",
		  "constant.other.key",
		  "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
		],
		"settings": {
		  "foreground": "#B2DFDB" // YENİ STRİNG RENGİ (Pastel Akuamarin)
		}
	  },
	  {
		"name": "CSS Property Name",
		"scope": [
		  "source.css support.type.property-name",
		  "source.sass support.type.property-name",
		  "source.scss support.type.property-name",
		  "source.less support.type.property-name",
		  "source.stylus support.type.property-name",
		  "source.postcss support.type.property-name",
		  "support.type.vendored.property-name.css",
		  "meta.property-name.css"
		],
		"settings": {
		  "foreground": "#FFFFFF"
		}
	  },
	  {
		  "name": "CSS Property Values, CSS Tags (non-class/id)",
		  "scope": [
			  "support.constant.property-value.css",
			  "meta.property-value.css",
			  "source.css.scss entity.name.tag",
			  "variable.parameter.keyframe-list.css",
			  "variable.parameter.url.scss"
		  ],
		  "settings": {
			  "foreground": "#B2DFDB" // YENİ STRİNG RENGİ (Pastel Akuamarin)
		  }
	  },
	  {
		"name": "HTML Attributes",
		"scope": [
		  "entity.other.attribute-name.html",
		  "text.html.basic entity.other.attribute-name"
		],
		"settings": {
		  "foreground": "#C5B4E3" // YENİ ANA VURGU (Pastel Lavanta)
		}
	  },
	  {
		"name": "CSS Classes and IDs Selectors",
		"scope": [
		  "entity.other.attribute-name.class.css",
		  "entity.other.attribute-name.id.css",
		  "meta.attribute-selector.scss entity.other.attribute-name.id",
		  "meta.attribute-selector.scss entity.other.attribute-name.class",
		  "variable.parameter.misc.css"
		],
		"settings": {
		  "foreground": "#C5B4E3" // YENİ ANA VURGU (Pastel Lavanta)
		}
	  },
	  {
		"name": "Inserted",
		"scope": ["markup.inserted", "markup.inserted.git_gutter"],
		"settings": {
		  "foreground": "#B2DFDB" // YENİ STRİNG RENGİ (Pastel Akuamarin)
		}
	  },
	  {
		"name": "Deleted",
		"scope": ["markup.deleted", "markup.deleted.git_gutter"],
		"settings": {
		  "foreground": "#E57373" // Pastel Kırmızı
		}
	  },
	  {
		"name": "Changed",
		"scope": ["markup.changed", "markup.changed.git_gutter"],
		"settings": {
		  "foreground": "#A0A0A0"
		}
	  },
	  {
		"name": "Regular Expressions",
		"scope": ["string.regexp"],
		"settings": {
		  "foreground": "#A0A0A0"
		}
	  },
	  {
		"name": "URL",
		"scope": ["*url*", "*link*", "*uri*"],
		"settings": {
		  "fontStyle": "underline"
		}
	  },
	  {
		"name": "Decorators",
		"scope": [
		  "tag.decorator.js entity.name.tag.js",
		  "tag.decorator.js punctuation.definition.tag.js"
		],
		"settings": {
		  "foreground": "#C5B4E3" // Ana Vurgu (Pastel Lavanta)
		}
	  },
	  { "name": "JSON Key - Level X", "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json", "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"], "settings": { "foreground": "#C5B4E3" }}, // YENİ ANA VURGU (Pastel Lavanta)
	  {
		"name": "Markdown - Heading",
		"scope": [
		  "markup.heading",
		  "markup.heading entity.name",
		  "markup.heading.markdown punctuation.definition.heading.markdown"
		],
		"settings": {
		  "foreground": "#C5B4E3", // YENİ ANA VURGU (Pastel Lavanta)
		  "fontStyle": "bold"
		}
	  },
	  { "name": "Markup - Italic", "scope": ["markup.italic"], "settings": { "fontStyle": "italic", "foreground": "#FFFFFF" }},
	  { "name": "Markup - Bold", "scope": ["markup.bold", "markup.bold string"], "settings": { "fontStyle": "bold", "foreground": "#FFFFFF" }},
	  { "name": "Markup - Underline", "scope": ["markup.underline"], "settings": { "fontStyle": "underline", "foreground": "#C5B4E3" }}, // YENİ ANA VURGU (Pastel Lavanta)
	  { "name": "Markdown - Blockquote", "scope": ["markup.quote punctuation.definition.blockquote.markdown", "markup.quote"], "settings": { "foreground": "#A0A0A0", "fontStyle": "italic" }},
	  { "name": "Markdown - Link Text", "scope": ["string.other.link.title.markdown", "markup.underline.link.markdown"], "settings": { "foreground": "#B2DFDB" }}, // YENİ STRİNG RENGİ (Pastel Akuamarin)
	  { "name": "Markdown - Link Anchor/URL", "scope": ["constant.other.reference.link.markdown", "markup.underline.link.markdown meta.link.reference"], "settings": { "foreground": "#C5B4E3" }} // YENİ ANA VURGU (Pastel Lavanta)
	]
  }