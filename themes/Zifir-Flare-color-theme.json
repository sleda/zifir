{
	"name": "Zifir Flare Pastel",
	"colors": {
	  // Editor
	  "editor.background": "#101010",
	  "editor.foreground": "#FFFFFF",
	  "editor.selectionBackground": "#FFFFFF25",
	  "editor.selectionHighlightBackground": "#FFFFFF25",
	  "editorLineNumber.foreground": "#505050",
	  "editorGroupHeader.tabsBackground": "#101010",
	  "editorWidget.background": "#101010",
	  "editorWarning.foreground": "#FFCC80", // Pastel Portakal (uyarı)
	  "editorError.foreground": "#F08080",   // Pastel Mercan
	  "editorOverviewRuler.border": "#101010",
	  "editorGutter.addedBackground": "#FFFACD", // YENİ PASTEL STRİNG
	  "editorGutter.deletedBackground": "#F08080",   // Pastel Mercan
	  "editorGutter.modifiedBackground": "#FFCC80", // Pastel Portakal (uyarı)
	  "diffEditor.insertedTextBackground": "#FFFACD1A", // Yeni pastel string alpha
	  "diffEditor.insertedLineBackground": "#FFFACD1A",
	  "diffEditor.removedTextBackground": "#F080801A",  // Yeni pastel hata alpha
	  "diffEditor.removedLineBackground": "#F080801A",
	  "editorInlayHint.foreground": "#A0A0A0",
	  "editorInlayHint.background": "#1C1C1C",
	  // Sidebar
	  "sideBar.background": "#101010",
	  "sideBarTitle.foreground": "#A0A0A0",
	  "sideBarSectionHeader.foreground": "#A0A0A0",
	  "sideBarSectionHeader.background": "#101010",
	  // Activity bar
	  "activityBar.background": "#101010",
	  "activityBar.foreground": "#A0A0A0",
	  "activityBarBadge.background": "#FFB347", // YENİ PASTEL ANA VURGU
	  "activityBarBadge.foreground": "#333333", // Koyu Gri Metin
	  // Title bar
	  "titleBar.activeBackground": "#101010",
	  "titleBar.inactiveBackground": "#101010",
	  "titleBar.activeForeground": "#7E7E7E",
	  "titleBar.inactiveForeground": "#707070",
	  // Tab
	  "tab.border": "#101010",
	  "tab.activeBackground": "#161616",
	  "tab.activeBorder": "#FFB347", // YENİ PASTEL ANA VURGU
	  "tab.inactiveBackground": "#101010",
	  // Status bar
	  "statusBar.debuggingForeground": "#FFFFFF",
	  "statusBar.debuggingBackground": "#B71C1C",
	  "statusBar.background": "#101010",
	  "statusBar.foreground": "#A0A0A0",
	  "statusBarItem.remoteBackground": "#FFB347", // YENİ PASTEL ANA VURGU
	  "statusBarItem.remoteForeground": "#333333", // Koyu Gri Metin
	  // Change explorer active item color
	  "list.activeSelectionForeground": "#FFB347", // YENİ PASTEL ANA VURGU
	  "list.inactiveSelectionBackground": "#232323",
	  // Global
	  "badge.background": "#FFB347", // YENİ PASTEL ANA VURGU
	  "badge.foreground": "#333333", // Koyu Gri Metin
	  "button.background": "#FFB347", // YENİ PASTEL ANA VURGU
	  "button.hoverBackground": "#FFBE5E", // Vurgu renginin biraz açığı
	  "button.foreground": "#333333", // Koyu Gri Metin
	  "focusBorder": "#FFB347", // YENİ PASTEL ANA VURGU
	  "icon.foreground": "#A0A0A0",
	  "input.background": "#1C1C1C",
	  "list.activeSelectionBackground": "#232323",
	  "list.hoverBackground": "#282828",
	  "list.errorForeground": "#F08080",   // Pastel Mercan
	  "list.highlightForeground": "#FFB347", // YENİ PASTEL ANA VURGU
	  "selection.background": "#666666",
	  // Brackets
	  "editorBracketHighlight.foreground1": "#A0A0A0",
	  "editorBracketHighlight.foreground2": "#A0A0A0",
	  "editorBracketHighlight.foreground3": "#A0A0A0",
	  "editorBracketHighlight.foreground4": "#A0A0A0",
	  "editorBracketHighlight.foreground5": "#A0A0A0",
	  "editorBracketHighlight.foreground6": "#A0A0A0",
	  "editorBracketHighlight.unexpectedBracket.foreground": "#F08080",
	  // Links
	  "textLink.foreground": "#FFB347", // YENİ PASTEL ANA VURGU
	  "textLink.activeForeground": "#FFBE5E", // Vurgu renginin biraz açığı
	  // Tooltips
	  "editorHoverWidget.background": "#161616",
	  "editorHoverWidget.border": "#282828",
	  // Scrollbar
	  "scrollbarSlider.background": "#34343480",
	  "scrollbarSlider.hoverBackground": "#343434",
	  // Settings
	  "settings.modifiedItemIndicator": "#FFB347" // YENİ PASTEL ANA VURGU
	},
	"tokenColors": [
	  {
		"name": "Comment",
		"scope": ["comment", "punctuation.definition.comment"],
		"settings": { "foreground": "#8b8b8b94" }
	  },
	  {
		"name": "Variables",
		"scope": ["variable", "string constant.other.placeholder"],
		"settings": { "foreground": "#FFFFFF" }
	  },
	  {
		"name": "Colors",
		"scope": ["constant.other.color"],
		"settings": { "foreground": "#FFFFFF" }
	  },
	  {
		"name": "Invalid",
		"scope": ["invalid", "invalid.illegal"],
		"settings": { "foreground": "#F08080" } // Pastel Mercan
	  },
	  {
		"name": "Keyword, Storage, Operator, Punctuation",
		"scope": [
		  "keyword", "storage.type", "storage.modifier", "keyword.operator",
		  "punctuation.definition.tag", "punctuation.separator", "punctuation.accessor"
		],
		"settings": { "foreground": "#A0A0A0" }
	  },
	  {
		"name": "Control Keywords",
		"scope": ["keyword.control"],
		"settings": { "foreground": "#A0A0A0" }
	  },
	  {
		"name": "Tag Name",
		"scope": ["entity.name.tag", "meta.tag.sgml"],
		"settings": { "foreground": "#FFB347" } // YENİ PASTEL ANA VURGU
	  },
	  {
		"name": "Function, Method, Class, Type Names",
		"scope": [
		  "entity.name.function", "variable.function", "support.function",
		  "entity.name.class", "entity.name.type", "support.class", "support.type",
		  "keyword.other.special-method", "support.other.namespace.use.php",
		  "meta.use.php", "support.other.namespace.php", "support.type.sys-types"
		],
		"settings": { "foreground": "#FFB347" } // YENİ PASTEL ANA VURGU
	  },
	  {
		"name": "Number, Boolean, Null, Constants",
		"scope": [
		  "constant.numeric", "constant.language.boolean", "constant.language.null",
		  "support.constant", "constant.character", "constant.escape", "keyword.other.unit"
		],
		"settings": { "foreground": "#FFFACD" } // YENİ PASTEL STRİNG
	  },
	  {
		"name": "String, Symbols, Keys",
		"scope": [
		  "string", "constant.other.symbol", "constant.other.key",
		  "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
		],
		"settings": { "foreground": "#FFFACD" } // YENİ PASTEL STRİNG
	  },
	  {
		"name": "CSS Property Name",
		"scope": [
		  "source.css support.type.property-name", "source.sass support.type.property-name",
		  "source.scss support.type.property-name", "source.less support.type.property-name",
		  "source.stylus support.type.property-name", "source.postcss support.type.property-name",
		  "support.type.vendored.property-name.css", "meta.property-name.css"
		],
		"settings": { "foreground": "#FFFFFF" }
	  },
	  {
		"name": "CSS Property Values, CSS Tags (non-class/id)",
		"scope": [
		  "support.constant.property-value.css", "meta.property-value.css",
		  "source.css.scss entity.name.tag", "variable.parameter.keyframe-list.css",
		  "variable.parameter.url.scss"
		],
		"settings": { "foreground": "#FFFACD" } // YENİ PASTEL STRİNG
	  },
	  {
		"name": "HTML Attributes",
		"scope": ["entity.other.attribute-name.html", "text.html.basic entity.other.attribute-name"],
		"settings": { "foreground": "#FFB347" } // YENİ PASTEL ANA VURGU
	  },
	  {
		"name": "CSS Classes and IDs Selectors",
		"scope": [
		  "entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css",
		  "meta.attribute-selector.scss entity.other.attribute-name.id",
		  "meta.attribute-selector.scss entity.other.attribute-name.class",
		  "variable.parameter.misc.css"
		],
		"settings": { "foreground": "#FFB347" } // YENİ PASTEL ANA VURGU
	  },
	  {
		"name": "Inserted",
		"scope": ["markup.inserted", "markup.inserted.git_gutter"],
		"settings": { "foreground": "#FFFACD" } // YENİ PASTEL STRİNG
	  },
	  {
		"name": "Deleted",
		"scope": ["markup.deleted", "markup.deleted.git_gutter"],
		"settings": { "foreground": "#F08080" } // Pastel Mercan
	  },
	  {
		"name": "Changed",
		"scope": ["markup.changed", "markup.changed.git_gutter"],
		"settings": { "foreground": "#A0A0A0" }
	  },
	  {
		"name": "Regular Expressions",
		"scope": ["string.regexp"],
		"settings": { "foreground": "#A0A0A0" }
	  },
	  {
		"name": "Decorators",
		"scope": ["tag.decorator.js entity.name.tag.js", "tag.decorator.js punctuation.definition.tag.js"],
		"settings": { "foreground": "#FFB347" } // YENİ PASTEL ANA VURGU
	  },
	  {
		"name": "JSON Key - Level X",
		"scope": [
			"source.json meta.structure.dictionary.json support.type.property-name.json",
			"source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
		],
		"settings": { "foreground": "#FFB347" } // YENİ PASTEL ANA VURGU
	  },
	  {
		"name": "Markdown - Heading",
		"scope": ["markup.heading", "markup.heading entity.name", "markup.heading.markdown punctuation.definition.heading.markdown"],
		"settings": { "foreground": "#FFB347", "fontStyle": "bold" } // YENİ PASTEL ANA VURGU
	  },
	  { "name": "Markup - Italic", "scope": ["markup.italic"], "settings": { "fontStyle": "italic", "foreground": "#FFFFFF" } },
	  { "name": "Markup - Bold", "scope": ["markup.bold", "markup.bold string"], "settings": { "fontStyle": "bold", "foreground": "#FFFFFF" } },
	  { "name": "Markup - Underline", "scope": ["markup.underline"], "settings": { "fontStyle": "underline", "foreground": "#FFB347" } },
	  { "name": "Markdown - Blockquote", "scope": ["markup.quote punctuation.definition.blockquote.markdown", "markup.quote"], "settings": { "foreground": "#A0A0A0", "fontStyle": "italic" } },
	  { "name": "Markdown - Link Text", "scope": ["string.other.link.title.markdown", "markup.underline.link.markdown"], "settings": { "foreground": "#FFFACD" } }, // YENİ PASTEL STRİNG
	  { "name": "Markdown - Link Anchor/URL", "scope": ["constant.other.reference.link.markdown", "markup.underline.link.markdown meta.link.reference"], "settings": { "foreground": "#FFB347" } } // YENİ PASTEL ANA VURGU
	]
  }